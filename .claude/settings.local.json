{
  "permissions": {
    "allow": [
      "Bash(for file in *.ts)",
      "Bash(do if ! grep -q \"prisma.moderationLog.create\" \"$file\")",
      "Bash(then echo \"Missing logs: $file\")",
      "Bash(fi)",
      "Bash(done)",
      "Bash(grep:*)",
      "Bash(rm:*)",
      "WebFetch(domain:discordtickets.app)",
      "WebFetch(domain:docs.tickettool.xyz)",
      "Bash(mkdir:*)",
      "Bash(pnpm add:*)",
      "Bash(rg:*)",
      "Bash(cp:*)"
    ],
    "deny": []
  }
}
